{
  "name": "PyDownTV",
  "version": "3.0.5",
  "manifest_version": 2,
  "description": "Descarga los vídeos de las webs de televisiones españolas",
  "homepage_url": "http://web.pydowntv.com",
  "icons": {
    "16": "icons/icon.png"
  },
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "PyDownTV"
  },
  "permissions": [
    "http://web.pydowntv.com/*",
    "tabs"
  ]
}